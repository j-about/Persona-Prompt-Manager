{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Default capabilities for the application",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"process:default",
		"os:default",
		"dialog:allow-open",
		"dialog:allow-save",
		{
			"identifier": "opener:allow-open-url",
			"allow": [{ "url": "https://buy.stripe.com/*" }]
		}
	]
}
